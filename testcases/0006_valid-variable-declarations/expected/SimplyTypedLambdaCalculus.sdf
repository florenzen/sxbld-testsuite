module SimplyTypedLambdaCalculus
 imports org/sugarj/sxbld/initGrammar
exports 
  (/)
context-free syntax
  "foo" -> SimplyTypedLambdaCalculusX    {cons("SXConsX0")}
  "foo" -> SimplyTypedLambdaCalculusGndX {cons("SXConsX0")}
  "bar" -> SimplyTypedLambdaCalculusY    {cons("SXConsY1")}
  "bar" -> SimplyTypedLambdaCalculusGndY {cons("SXConsY1")}
context-free syntax
  SXMetaVarX -> SimplyTypedLambdaCalculusX {prefer, cons("SXMetaVarX")}
  SXMetaVarY -> SimplyTypedLambdaCalculusY {prefer, cons("SXMetaVarY")}
lexical syntax
  "x" [a-zA-Z0-9]* -> SXMetaVarX 
  "y" [a-zA-Z]*    -> SXMetaVarY 